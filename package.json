{
  "name": "cirsqu-auth",
  "version": "0.0.1",
  "description": "This api is for user authentication.",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "cross-env NODE_ENV=production node dist",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --pretty --transpile-only src/index.ts dev",
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register src/routes/**/*.spec.ts --no-timeout",
    "test:all": "cross-env NODE_ENV=test mocha -r ts-node/register src/**/*.spec.ts --no-timeout",
    "test:model": "cross-env NODE_ENV=test mocha -r ts-node/register src/models/**/*.spec.ts --no-timeout",
    "test:service": "cross-env NODE_ENV=test mocha -r ts-node/register src/services/**/*.spec.ts --no-timeout",
    "format": "prettier --write **/*.{ts,json}"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.19.0",
    "amqplib-as-promised": "^4.1.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "express-jsdoc-swagger": "^1.6.7",
    "file-type": "16.3.0",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "mongoose": "^6.2.10",
    "morgan": "^1.10.0",
    "sharp": "^0.30.3",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@types/sharp": "^0.30.1",
    "axios": "^0.26.1",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
